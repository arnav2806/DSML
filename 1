import pandas as pd

df = pd.read_csv('Titanic.csv')

print(df.head())

#selecting
print(df[['Pclass','Ticket']].head())
print(df.iloc[0:10])
print(df.loc[10,'SibSp'])
print(df.loc[:,['PassengerId','Sex']])

print(df.sort_values(by = 'Survived'))
print(df.sort_index(axis=1,ascending=False))

print(df.describe())
print(df.describe(include=['object']))

# Check data types
print(df.dtypes)
